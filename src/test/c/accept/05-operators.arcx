class Mordor
{
		 mountDoom : Function<void> = ()
		 {
				 sam : int = 1;
				 frodo : int = 4;
				 gollum : int = 4;
		 
				 print(sam + frodo); #Suma, Imprime 5.
				 print(sam * frodo); #Multiplicacion, Imprime 4.
				 print(frodo ** gollum); #Potencia, Imprime 16.
				 
				 print(frodo == gollum); #Igual, compara dirección de memoria, Imprime False.
				 print(frodo != gollum ); #Distinto, compara dirección de memoria, Imprime True.
		 }
}